<openerp>
    <data>
    	<record model="ir.ui.view" id="current_stock_report_free_wizard_view">
            <field name="name">current.stock.report.free.wizard.view</field>
            <field name="model">current.stock.report.free.wizard</field>
            <field name="arch" type="xml">
                <form string="Current Stock Report Free Product" version="9.0">
                	<group>
						<field name="location_id" required="1"/>
					</group>
					<footer>
                        <button string="Print" name="do_print" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    	
        <record id="action_current_stock_report_free_wizard" model="ir.actions.act_window">
            <field name="name">Current Stock Report Free Product</field>
            <field name="res_model">current.stock.report.free.wizard</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    	<menuitem
            id="menu_inventory_report_free" name="Current Stock Report Free Product"
    		action="action_current_stock_report_free_wizard"
            parent="point_of_sale.menu_point_rep" sequence="2"/>
    </data>
</openerp>