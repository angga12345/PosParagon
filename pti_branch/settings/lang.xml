<openerp>
    <data noupdate="1">
        <record id="custom_lang_en" model="ir.model.data">
            <field name="name">lang_en</field>
            <field name="noupdate">TRUE</field>
            <field name="module">pti_branch</field>
            <field name="model">res.lang</field>
            <field name="res_id" model="res.lang" search="[('code', '=', 'en_US')]"/>
        </record>
        <record id="lang_en" model="res.lang">
            <field name="grouping">[3,0]</field>
            <field name="direction">ltr</field>
            <field name="active">True</field>
            <field name="code">en_US</field>
            <field name="date_format">%m/%d/%Y</field>
            <field name="decimal_point">,</field>
            <field name="translatable">True</field>
            <field name="iso_code"></field>
            <field name="time_format">%H:%M:%S</field>
            <field name="thousands_sep">.</field>
        </record>
    </data>
</openerp>