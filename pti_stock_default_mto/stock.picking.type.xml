<openerp>
    <data>
        <record id="picking_goods" model="stock.picking.type">
            <field name="name">Picking Goods</field>
            <field name="code">internal</field>
            <field name="apply_mto">False</field>
            <field name="sequence_id" model="ir.sequence" search="['|','|','|',('prefix','=','NDC\\INT\\'),('prefix','=','NDC/INT/'),('prefix','=','WH\\INT\\'),('prefix','=','WH/INT/')]"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="return_picking_type_id" ref="stock.picking_type_in"/>
            <field name="default_location_src_id" ref="stock.stock_location_stock"/>
            <field name="default_location_dest_id" ref="ndc_staging"/>
       </record>
       
       <record id="picking_quarantine" model="stock.picking.type">
            <field name="name">Picking Quarantine</field>
            <field name="code">internal</field>
            <field name="apply_mto">False</field>
            <field name="sequence_id" model="ir.sequence" search="['|','|','|',('prefix','=','NDC\\INT\\'),('prefix','=','NDC/INT/'),('prefix','=','WH\\INT\\'),('prefix','=','WH/INT/')]"/>
            <field name="warehouse_id" ref="stock.warehouse0"/>
            <field name="default_location_src_id" ref="ndc_transit_quarantine"/>
            <field name="default_location_dest_id" ref="ndc_quarantine"/>
       </record>
    </data>
</openerp>