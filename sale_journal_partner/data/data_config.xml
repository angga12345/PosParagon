<openerp>
    <data noupdate="1">
    	<record id="sequence_parama_out" model="ir.sequence">
            <field name="name">Parama Out</field>
            <field name="code">parama.out</field>
            <field name="prefix">PRM/OUT/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        <record id="sequence_parama_in" model="ir.sequence">
            <field name="name">Parama In</field>
            <field name="code">parama.in</field>
            <field name="prefix">PRM/IN/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        
        <record id="sequence_parama_sale_oerder" model="ir.sequence">
            <field name="name">Parama Sales Order</field>
            <field name="code">prm.sale.orders</field>
            <field name="prefix">PRM/SO</field>
            <field eval="1" name="number_increment"/>
            <field name="padding">3</field>
        </record>
        
        <record id="sequence_parama_invoice_number" model="ir.sequence">
            <field name="name">Parama Invoice Number</field>
            <field name="code">invoice.number.parama.sequence</field>
            <field name="prefix">%(range_year)s</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        <record id="sequence_parama_cancel_invoice_number" model="ir.sequence">
            <field name="name">Parama Canceled Invoice Number</field>
            <field name="code">invoice.cancel.number.parama.sequence</field>
            <field name="prefix">%(range_year)s</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        
        <record id="sequence_parama_debit_number" model="ir.sequence">
            <field name="name">Parama Debit Number</field>
            <field name="code">debit.number.parama.sequence</field>
            <field name="prefix">%(range_year)s</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        <record id="sequence_parama_cancel_debit_number" model="ir.sequence">
            <field name="name">Parama Canceled Debit Number</field>
            <field name="code">debit.cancel.number.parama.sequence</field>
            <field name="prefix">%(range_year)s</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        
        <record id="sequence_parama_credit_number" model="ir.sequence">
            <field name="name">Parama Credit Number</field>
            <field name="code">credit.number.parama.sequence</field>
            <field name="prefix">%(range_year)s</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
        <record id="sequence_parama_cancel_credit_number" model="ir.sequence">
            <field name="name">Parama Canceled Credit Number</field>
            <field name="code">credit.cancel.number.parama.sequence</field>
            <field name="prefix">%(range_year)s</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
    </data>
</openerp>